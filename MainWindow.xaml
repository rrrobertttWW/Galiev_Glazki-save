<Window x:Class="Galiev_Глазки_save.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Galiev_Глазки_save"
        mc:Ignorable="d"
        Title="Galiev" Height="450" Width="800" FontFamily="Century Gothic" Icon="res/Глазки-save.ico" MinHeight="815" MinWidth="1350">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="res/Глазки-save.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Глазки-save" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Name="BtnBack" Content="Назад" Click="BtnBack_Click" Grid.Row="0" HorizontalAlignment="Right"></Button>
    </Grid>
</Window>
